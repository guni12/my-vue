{
    "name": "me-vue",
    "version": "5.0.0",
    "private": true,
    "description": "Me-page for Guni's Ramverk2, front-end",
    "scripts": {
        "prebuild": "node ./node_modules/rimraf/bin.js ./node_modules/.cache",
        "build": "vue-cli-service build --modern",
        "serve": "vue-cli-service serve --host localhost",
        "lint": "vue-cli-service lint",
        "deploy": "npm run build && rsync -av dist/* deploy@guni.me:/var/www/my-vue.guni.me/html/",
        "test": "node ./node_modules/mocha/bin/_mocha test/**/*.js",
        "test2": "mocha test/**/*.js"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "vue": "^2.6.6",
        "vue-router": "^3.0.2",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.4.0",
        "@vue/cli-plugin-eslint": "^3.4.0",
        "@vue/cli-service": "^3.4.0",
        "@vue/eslint-config-prettier": "^3.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.13.0",
        "eslint-plugin-vue": "^5.0.0-0",
        "http-server": "^0.11.1",
        "mocha": "^4.1.0",
        "rimraf": "^2.6.3",
        "selenium-webdriver": "^3.6.0",
        "vue-template-compiler": "^2.6.6"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
